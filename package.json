{
  "name": "nuxtr-monorepo-projectswitcher",
  "displayName": "Nuxtr Monorepo Project Switcher",
  "description": "Easily switch between multiple Nuxt projects in a monorepo workspace for the Nuxtr extension",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/philippedasilva-orizone/nuxtr-monorepo-projectswitcher.git"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "nuxtr.switchRepo",
        "title": "Nuxtr: Switch Monorepo Project"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/node": "16.x",
    "typescript": "^4.7.4"
  }
}